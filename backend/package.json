{
  "name": "backend",
  "version": "1.0.0",
  "description": "ILike Backend service",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node dist/server.js",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "prettier:check": "npm run prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\""
  },
  "author": "Ezeh Valentine",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/mongodb": "^3.5.34",
    "@types/mongoose": "^5.7.1",
    "bodybuilder": "^2.2.21",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dot": "^1.1.3",
    "dotenv": "^8.2.0",
    "env": "0.0.2",
    "express": "^4.17.1",
    "indicative": "^7.4.4",
    "mongoose": "^5.8.11",
    "uuid": "^7.0.2"
  }
}
